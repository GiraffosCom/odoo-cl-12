<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="view_order_form" model="ir.ui.view">
		<field name="name">view_order_form.form</field>
		<field name="model">sale.order</field>
		<field name="inherit_id" ref="sale.view_order_form"/>
		<field name="arch" type="xml">
			<field name="partner_id" position="before">
                  <field name="saleordertype_id" attrs="{'required': ['true']}"/>
			</field>
			<field name="partner_id" position="after">
                  <field name="cenabast_purchase_order" attrs="{'invisible': [('saleordertype_id','!=',5)]}"/>
                  <field name="cenabast_sales_order" attrs="{'invisible': [('saleordertype_id','!=',5)]}"/>
			</field>

        </field>
	</record>
</odoo>